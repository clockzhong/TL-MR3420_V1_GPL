# Remove by TanZY for WA730RE
#rm -rf $RPM_DIR/Wzd*
#rm -rf $HELP_DIR/Wzd*

rm -rf $RPM_DIR/VirtualServer*
rm -rf $HELP_DIR/VirtualServerHelpRpm.htm

rm -rf $RPM_DIR/UpnpCfgRpm.htm
rm -rf $HELP_DIR/UpnpCfgHelpRpm.htm

rm -rf $RPM_DIR/DMZRpm.htm
rm -rf $HELP_DIR/DMZHelpRpm.htm

rm -rf $RPM_DIR/QoS*
rm -rf $HELP_DIR/QoS*

rm -rf $RPM_DIR/PPP*
rm -rf $HELP_DIR/PPP*

rm -rf $RPM_DIR/PPTPCfgRpm.htm
rm -rf $HELP_DIR/PptpCfgHelpRpm.htm

rm -rf $RPM_DIR/Wan*
rm -rf $HELP_DIR/Wan*

rm -rf $RPM_DIR/StaticRoute*
rm -rf $HELP_DIR/StaticRoute*

rm -rf $RPM_DIR/SysRouteTable*
rm -rf $HELP_DIR/SysRouteTable*

rm -rf $RPM_DIR/SpecialApp*
rm -rf $HELP_DIR/SpecialApp*

rm -rf $RPM_DIR/L2TPCfgRpm.htm
rm -rf $HELP_DIR/L2tpCfgHelpRpm.htm

rm -rf $RPM_DIR/DynDdnsRpm.htm
rm -rf $HELP_DIR/DynDdnsHelpRpm.htm

rm -rf $RPM_DIR/NoipDdnsRpm.htm
rm -rf $HELP_DIR/NoipDdnsHelpRpm.htm

rm -rf $RPM_DIR/MacCloneCfgRpm.htm
rm -rf $HELP_DIR/MacCloneCfgHelpRpm.htm

rm -rf $RPM_DIR/SystemStatisticRpm.htm
rm -rf $HELP_DIR/SystemStatisticHelpRpm.htm

rm -rf $RPM_DIR/BasicSecurityRpm.htm
rm -rf $RPM_DIR/BPACfgRpm.htm
rm -rf $RPM_DIR/LocalManageControlRpm.htm
rm -rf $RPM_DIR/ManageControlRpm.htm

rm -rf $HELP_DIR/BpaCfgHelpRpm.htm
rm -rf $HELP_DIR/DdnsAddComexeHelpRpm.htm
rm -rf $HELP_DIR/BasicSecurityHelpRpm.htm
rm -rf $HELP_DIR/LocalManageControlHelpRpm.htm
rm -rf $HELP_DIR/ManageControlHelpRpm.htm

rm -rf $RPM_DIR/DateTimeCfgRpm.htm
rm -rf $HELP_DIR/DateTimeCfgHelpRpm.htm

for i in $MOD_NAME
do
	rm -rf $KMOD_DIR/$i.ko
done
