#!/bin/bash

RPM_DIR=$INSTALL_ROOT_FOR_BOARD/web/userRpm
HELP_DIR=$INSTALL_ROOT_FOR_BOARD/web/help
KMOD_DIR=$INSTALL_ROOT_FOR_BOARD/lib/modules/2.6.15/kernel
MOD_NAME='cls_basic cls_fw sch_htb sch_prio sch_sfq iptable_nat ipt_conntrack ipt_iprange ipt_mac ipt_MARK ipt_MASQUERADE ipt_multiurl ipt_string ipt_TCPMSS ipt_time ipt_TRIGGER ip_conntrack_ftp ip_conntrack_h323 ip_conntrack_pptp ip_conntrack_tftp ip_nat_ftp ip_nat_h323 ip_nat ip_nat_pptp ip_nat_tftp statistics ts_kmp pppol2tp pptp harmony wlan_warn ip_conntrack_rtsp ip_nat_rtsp'

rm -rf $RPM_DIR/AccessCtrl*
